@article{ WOS:000357843000009,
Author = {Wijesekara, R. G. Saman and Alfafara, Catalino G. and Matsumura,
   Masatoshi},
Title = {Evaluation of bio-acetal as a sustainable alternative jet fuel},
Journal = {JOURNAL OF THE NATIONAL SCIENCE FOUNDATION OF SRI LANKA},
