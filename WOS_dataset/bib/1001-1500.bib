@article{ WOS:000765061900005,
Author = {Miller, Jacob H. and Hafenstine, Glenn R. and Nguyen, Hannah H. and
   Vardon, Derek R.},
Title = {Kinetics and Reactor Design Principles of Volatile Fatty Acid
   Ketonization for Sustainable Aviation Fuel Production},
Journal = {INDUSTRIAL \& ENGINEERING CHEMISTRY RESEARCH},
Year = {2022},
Volume = {61},
