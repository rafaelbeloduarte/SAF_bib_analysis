@article{ WOS:000882423100003,
Author = {Choi, Il-Ho and Lee, Hye-Jin and Hwang, Kyung-Ran},
Title = {Hydro-isomerization of n-dodecane on Pt-La/Beta catalyst for the
   production of high quality bio-jet fuel: Effect of La addition},
Journal = {MICROPOROUS AND MESOPOROUS MATERIALS},
Year = {2022},
Volume = {346},
Month = {DEC},
Abstract = {A series of Pt-La/B38 catalysts were prepared for low-temperature
   isomerization of n-dodecane and the effects of La addition on the
   textural properties, metal dispersion, acid properties, and catalytic
   performance were investigated. La co-impregnated with Pt on B38
   significantly reduced the Pt size and notably enhanced the uniformity in
   Pt size. A higher ratio of accessible Pt and medium-strength Bronsted
   acid sites and a shorter distance between two Pt particles of
   Pt-La10/B38 resulted in the maximum iso-C12 yield (59.2-56.2\%) in the
   range of 200-250 degrees C, due to the reasonable arrangement of active
   sites caused by La loading.},
