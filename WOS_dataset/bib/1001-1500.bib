@article{ WOS:000824080700001,
Author = {Yoo, Eunji and Lee, Uisung and Wang, Michael},
Title = {Life-Cycle Greenhouse Gas Emissions of Sustainable Aviation Fuel through
   a Net-Zero Carbon Biofuel Plant Design},
