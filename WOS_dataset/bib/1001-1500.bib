@article{ WOS:000605477500021,
Author = {Yang, Lingmei and Xing, Shiyou and Luo, Wen and Yang, Gai xiu and Wang,
   Zhongming and Lv, Pengmei},
Title = {Fabrication of sea urchin-like hierarchical porous SAPO-11 molecular
   sieves toward hydrogenation of lipid to jet fuel},
