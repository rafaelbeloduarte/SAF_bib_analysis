@article{ WOS:000742148000001,
Author = {Ramesh, Arumugam and Shanthi, Kannan and Minh-Tri Nguyen-Le},
Title = {NiMoS over Ti-incorporated mesoporous silicates for the hydrotreating of
   non-edible oils to bio-jet fuels},
Journal = {MOLECULAR CATALYSIS},
Year = {2022},
Volume = {518},
Month = {JAN},
Abstract = {Titanium incorporation over silicates such as SBA-15, KIT-6, FSM-16, and
   SBA-16 with Si/Ti 10 ratio was synthesized by a hydrothermal method.
   NiMoS over Ti-incorporated silicates was prepared at 180 degrees C for
   12 h by a hydrothermal method using nickel nitrate, ammonium
   heptamolybdate, and thiourea as a respective precursor for Ni, Mo and S.
   Deep investigations were performed for hydrodeoxygenation (HDO) of
   karanja oil through optimization of different parameters at 225 degrees
   C - 375 degrees C under 5-35 bar pressure, WHSV 1-3 h(-1), with TOS up
   to 54th h. Detailed studies were conducted for the same reaction
   parameters while NiMoS over Ti-incorporated silicate catalysts were
   constantly fixed for material preparation. Among different catalysts,
   the NiMoS/Ti-K exhibited superior catalytic activity towards karanja oil
   conversion of about above 90\% and selectivity of 50\% for > C18. The
   mass transfer consideration of study also performed in detail. The
   activity and physico-chemical techniques were correlated with one
   another, thereby confirmed NiMoS/Ti-K as the potential candidate for
   superior catalytic activity.},
