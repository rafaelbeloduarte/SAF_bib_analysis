@article{ WOS:000864536000013,
Author = {Lee, Sangmin and Lee, Yu Rim and Lee, Won-Heong and Lee, Soo Youn and
   Moon, Myounghoon and Park, Gwon Woo and Min, Kyoungseon and Lee, Juah
   and Lee, Jin-Suk},
Title = {Valorization of CO2 to beta-farnesene in Rhodobacter sphaeroides},
Journal = {BIORESOURCE TECHNOLOGY},
Year = {2022},
Volume = {363},
Month = {NOV},
Abstract = {The valorization of CO2 into valuable products is a sustainable strategy
   to help overcome the climate crisis. In particular, biological
   conversion is attractive as it can produce long-chain hydrocarbons such
   as terpenoids. This study reports the high yield of beta-farnesene
   production from CO2 by expressing heterologous beta-farnesene synthase
   (FS) into Rhodobacter sphaeroides. To increase the expression of FS, a
   strong active promoter and a ribosome binding site (RBS) were
   engineered. Moreover, beta-farnesene production was improved further
   through the supply of exogenous antioxidants and additional nutrients.
   Finally, beta-farnesene was produced from CO2 at a titer of 44.53 mg/L
   and yield of 234.08 mg/g, values that were correspondingly 23 times and
   46 times higher than those from the initial production of
   beta-farnesene. Altogether, the results here suggest that the
   autotrophic production of beta-farnesene can provide a starting point
   for achieving a circular carbon economy.},
