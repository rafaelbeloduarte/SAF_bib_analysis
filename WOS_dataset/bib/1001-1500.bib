@article{ WOS:000691290400003,
Author = {Kuyukina, Maria S. and Ivshina, Irena B. and Krivoruchko, V, Anastasiya
   and Peshkur, Tatyana A. and Cunningham, Colin J.},
Title = {Improvement of jet fuel contaminated water treatment in a fluidized-bed
   bioreactor by introducing nickel nanoparticles},
Journal = {INTERNATIONAL BIODETERIORATION \& BIODEGRADATION},
Year = {2021},
Volume = {164},
Month = {OCT},
Abstract = {A biotreatment of jet fuel contaminated water was performed in the
   fluidized-bed bioreactor (FBB) amended with Rhodococcus opacus and
   Rhodococcus ruber association co-immobilized into polymeric cryogel in
   combination with nickel nanoparticles. Laboratory FBBs were continuously
   operating for 4 weeks, providing 80-100\% removal of n-alkanes and
   iso-alkanes, 40-60\% removal of benzene derivates and naphthalenes from
   contaminated water. Respirometry tests showed that poly(vinyl alcohol)
   cryogel-encapsulated bacterial cells were resistant to metal
   nanoparticles and fuel constituents. Using fuel bio-oxidation
   stoichiometry, a dissolved oxygen level in FBBs was suggested as a
   limiting factor. Pretreatment of water with Ni nanoparticles contributed
   to a 10\% increase in the efficiency of hydrocarbon biodegradation,
   presumably as a result of isomerization and oxidation. The novel
   integrated approach involving nanometal activation and biodegradation
   can be exploited for enhancing the treatment of hydrocarbon-containing
   wastewaters.},
