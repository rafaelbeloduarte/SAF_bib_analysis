@article{ WOS:000734926500001,
Author = {Robertson, Keven Alan and English, Burton C. and Clark, Christopher D.
   and Thompson, Jada M. and Jensen, Kimberly L. and Menard, Robert Jamey
   and Labbe, Nicole},
Title = {Optimal N Application Rates on Switchgrass for Producers and a
   Biorefinery},
