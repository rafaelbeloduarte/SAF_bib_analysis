Author = {Oldani, Anna and Valco, Daniel and Min, Kyungwook and Edwards, James and
   Kweon, Chol-Bum and Allen, Casey and Lee, Tonghun},
Title = {Conventional and Bio-Derived Jet Fuel Surrogate Modeling in Low
   Temperature and Lean Combustion},
