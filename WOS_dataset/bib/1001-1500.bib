@article{ WOS:000876005900001,
Author = {Gualtieri, Maurizio and Berico, Massimo and Grollino, Maria and Cremona,
   Giuseppe and La Torretta, Teresa and Malaguti, Antonella and Petralia,
   Ettore and Stracquadanio, Milena and Santoro, Massimo and Benassi,
