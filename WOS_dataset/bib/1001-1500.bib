@article{ WOS:000927280700001,
Author = {Gronau, Steven and Hoelzen, Julian and Mueller, Tobias and
   Hanke-Rauschenbach, Richard},
Title = {Hydrogen-powered aviation in Germany: A macroeconomic perspective and
   methodological approach of fuel supply chain integration into an
   economy-wide dataset},
