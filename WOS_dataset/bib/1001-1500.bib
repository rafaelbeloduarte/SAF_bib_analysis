@article{ WOS:000701196200002,
Author = {Pearson, Richard and Coe, Andrew and Paterson, James},
Title = {Innovation in Fischer-Tropsch: A Sustainable Approach to Fuels
   Production A cost-effective method of converting any carbon source into
   high-quality liquid hydrocarbon fuels},
Journal = {JOHNSON MATTHEY TECHNOLOGY REVIEW},
Year = {2021},
Volume = {65},
Number = {3},
Pages = {395-403},
Month = {JUL},
Abstract = {A sustained global effort is required over the next few decades to
   reduce greenhouse gas emissions, in order to address global warming as
   society seeks to deliver the Paris Agreement temperature goals. The
   increasing availability of renewable electricity will reduce our
   reliance on fossil fuels. However, some applications, such as long-haul
   aviation, are particularly challenging to decarbonise. The conversion of
   waste, biomass or existing CO2 emissions into sustainable fuels via
   Fischer-Tropsch (FT) synthesis offers one solution to this problem. This
   paper describes some of the challenges associated with this route to
   these alternative fuels and how Johnson Matthey and bp have solved them.},
