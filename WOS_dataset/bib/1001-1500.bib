@article{ WOS:000622220700004,
Author = {Tsai, Shu-Yao and Su, Jia-Wei and Tong, Jing-Ting and Lin, Chun-Ping},
Title = {Evaluation of suitable mixture proportion compatibility of aviation fuel
   JET A-1 blended with butanol isomers},
Journal = {JOURNAL OF THERMAL ANALYSIS AND CALORIMETRY},
Year = {2021},
Volume = {144},
Number = {2},
Pages = {403-413},
Month = {APR},
Abstract = {This study focuses on the compatibility and thermal reactivity of blends
   of aviation kerosene JET A-1 with four kinds of butanol. This
   prospective research predicts a suitable ratio of butanol isomers that
   can be added to JET A-1 for turbojets. Thus, this study was performed to
   establish the physical stability of different blending ratios at low
   temperature, high-temperature gasification stability, gas expansion
   enthalpy, and heat of combustion of blended aviation fuel. The blend
   proportion compatibility and thermal reactive stability of the low- and
   high-temperature physical characteristics and the combustion properties
   were studied by conducting refrigerated cooling system-differential
   scanning calorimetry (RCS90-DSC) and oxygen bomb calorimetry. We
   obtained two kinds of butanol isomers, n-butanol and sec-butanol, with
   the highest and best ratio of addition of 20\% to JET A-1, they include
   the compatibility parameters, such as the onset temperature of freezing
   characteristics, the onset gasification temperature, the expansion
   enthalpy (selected nonthermal heating rate of 4 degrees C min(-1)), and
   heat values are -73.23 and -64.87 degrees C, 58.61 and 22.92 degrees C,
   52.42 and.52.99 kJ kg(-1), and 43.46 and 42.98 MJ kg(-1), respectively.
   This research was developed to preliminarily assess the feasibility and
   compatibility of the suitable addition of bio-oil or bio-butanol to
   aviation fuel for use in aircraft engines to reduce engine testing and
   energy consumption.},
