@article{ WOS:000564287300008,
Author = {Fujitsuka, Hiroyasu and Yagita, Yusuke and Saika, Takashi and Ohshio,
   Nobuyasu and Iitsuka, Yoshihiro and Nakajima, Nobumasa and Tago, Teruoki},
Title = {Development of Pt/Zeolite for Selective Conversion of n-Decane and
   n-Dodecane to i-C6-7 and i-C7-9},
Journal = {JOURNAL OF THE JAPAN PETROLEUM INSTITUTE},
