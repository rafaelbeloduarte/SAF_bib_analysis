@article{ WOS:000792647500001,
Author = {Domenech, Pablo and Pogrebnyakov, Ivan and Nielsen, Alex T. and
   Riisager, Anders},
Title = {Catalytic production of long-chain hydrocarbons suitable for jet-fuel
   use from fermentation-derived oxygenates},
