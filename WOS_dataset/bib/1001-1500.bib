@article{ WOS:000900203600001,
Author = {Boehm, Randall C. C. and Hauck, Franchesca and Yang, Zhibin and
   Wanstall, C. Taber and Heyne, Joshua S. S.},
Title = {Error quantification of the Arrhenius blending rule for viscosity of
   hydrocarbon mixtures},
