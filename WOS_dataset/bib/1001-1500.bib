@article{ WOS:000872165300001,
Author = {Maki-Arvela, Paivi and Aho, Atte and Simakova, Irina and Murzin, Dmitry
   Yu},
Title = {Sustainable Aviation Fuel from Syngas through Higher Alcohols},
Journal = {CHEMCATCHEM},
