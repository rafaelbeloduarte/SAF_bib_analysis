Author = {Mishchenko, Andriy and Dalkiran, Alper and Novakovska, Iryna and
   Skrypnyk, Liliia and Ishchenko, Nataliia},
Title = {Environmentally sustainable airport development: Ukrainian case of
   decarbonization},
