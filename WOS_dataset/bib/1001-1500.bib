@article{ WOS:000429285800049,
Author = {Ju, Chao and Wang, Meng and Huang, Yong and Fang, Yunming and Tan,
   Tianwei},
Title = {High-Quality Jet Fuel Blend Production by Oxygen-Containing Terpenoids
   Hydroprocessing},
