@article{ WOS:000942297600047,
Author = {Aydin, Sinem Gurkan and Ozgen, Arzu},
Title = {Bio-Based Jet Fuel Production by Transesterification of Nettle Seeds},
Journal = {ENGINEERING TECHNOLOGY \& APPLIED SCIENCE RESEARCH},
Year = {2023},
