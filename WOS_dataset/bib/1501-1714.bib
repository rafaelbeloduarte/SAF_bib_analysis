@article{ WOS:000735381600002,
Author = {Merkisz, Jerzy and Jasinski, Remigiusz and Legowik, Anna and Olejnik,
   Aleksander},
Title = {EXHAUST EMISSIONS OF JET ENGINES POWERED BY BIOFUEL},
Journal = {TRANSPORT PROBLEMS},
Year = {2021},
Volume = {16},
Number = {4},
Pages = {199-206},
Abstract = {Biofuel use is one of the basic strategies to reduce the negative impact
   of aviation on the environment. Over the past two decades, a number of
   biofuels produced from plants, lubricants and maintenance products have
   been developed and introduced. New fuels must have specific
   physicochemical parameters and meet stringent standards. This article
   presents a comparative analysis of the exhaust emissions measurement
   results from jet engines powered by traditional aviation kerosene and
   its blends with ATJ (Alcohol to Jet) biofuel. The concentrations of
   carbon dioxide, carbon monoxide and hydrocarbons were measured.
   Measurements were conducted in laboratory conditions for various engine
   load values. Based on the analysis, it was found that the use of biofuel
   increases the concentration of carbon monoxide and hydrocarbons in the
   exhaust gas relative to aviation kerosene. The use of biofuel did not
   result in an increase in fuel consumption and related carbon dioxide
   emissions. Based on the conducted research, it was found that biofuel
   use did not affect the ecological properties of the engine
   significantly. In addition, a correlation analysis of the measurement
   results from both engines was carried out.},
