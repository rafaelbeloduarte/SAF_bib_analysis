Title = {Universal Range Equation for Unconventional Aircraft Concepts},
Booktitle = {2022 IEEE/AIAA TRANSPORTATION ELECTRIFICATION CONFERENCE AND ELECTRIC
   AIRCRAFT TECHNOLOGIES SYMPOSIUM (ITEC+EATS 2022)},
