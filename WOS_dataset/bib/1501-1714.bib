@article{ WOS:000809391600005,
Author = {Huete, Jon and Nalianda, Devaiah and Zaghari, Bahareh and Pilidis,
   Pericles},
Title = {A Strategy to Decarbonize Civil Aviation A phased innovation approach to
   hydrogen technologies},
Journal = {IEEE ELECTRIFICATION MAGAZINE},
Year = {2022},
Volume = {10},
Number = {2},
Pages = {27-33},
Month = {JUN},
Abstract = {GREENING AVIATION IS KEY TO OUR global future. However, reducing
   aircraft operations would severely impact worldwide economics.
   Therefore, it is imperative that radical and green propulsion
   alternatives be developed quickly. The most likely approaches to succeed
   in this grand challenge are hydrogen (H-2) and electrification, which
   hold the promise of complete decarbonization. H-2 is a promising
   solution for a fully sustainable aviation future, via hybrid/fuel cell
   technologies for short-to-medium-range and direct H-2 combustion in
   turbine engines for longer missions. However, the huge financial
   investments required to make this a reality will not be considered until
   several technical challenges can be overcome. Given the pressure to
   start decarbonizing soon, we propose a philosophy of innovation waves on
   the basis that to accelerate the implementation of the necessary
   technologies, it would be beneficial to rapidly develop suboptimal
   solutions and improve them through time and experience.},
