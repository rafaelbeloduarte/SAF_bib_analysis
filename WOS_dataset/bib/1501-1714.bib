Editor = {Faaij, APC and Baxter, D and Grassi, A and Helm, P},
Title = {POWER AND BIOMASS-TO-LIQUID (PBTL): A PROMISING APPROACH TO PRODUCE
   BIOFUELS USING ELECTRICITY},
