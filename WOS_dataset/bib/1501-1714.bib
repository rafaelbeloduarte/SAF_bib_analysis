@article{ WOS:000484813900006,
Author = {Baumi, Jonathan and Bertosse, Caroline M. and Guedes, Carmen L. B.},
Title = {Obtaining of Additive for Aviation Kerosene from a Co-Product of the
   Sugar and Alcohol Industry},
Journal = {REVISTA VIRTUAL DE QUIMICA},
