@article{ WOS:000860590600017,
Author = {Palies, Paul P.},
Title = {Hydrogen Thermal-Powered Aircraft Combustion and Propulsion System},
Journal = {JOURNAL OF ENGINEERING FOR GAS TURBINES AND POWER-TRANSACTIONS OF THE
   ASME},
