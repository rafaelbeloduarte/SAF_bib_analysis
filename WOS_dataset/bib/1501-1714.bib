@article{ WOS:000836219500010,
Author = {Hraiech, I and Riahi, Z. and Sautet, J-Ch and Mhimid, A.},
Title = {Effects of H-2 Addition and CO2 Dilution on the Methane-Air Diffusion
   Flame in a Coflow Burner},
